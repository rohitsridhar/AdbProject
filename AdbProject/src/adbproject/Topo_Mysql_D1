select sk.OGR_FID,co.OGR_FID from places co,places sk where equals(co.shape,sk.shape)=true limit 50;
select  sk.gid,co.gid from Poly_veg co,Poly_veg sk where equals(co.shape,sk.shape)=true limit 50;
select  sk.gid,co.gid from Poly_veg co,Poly_veg sk where overlaps(co.shape,sk.shape)=true limit 50;
select sk.OGR_FID,co.OGR_FID from places co,places sk where disjoint(co.shape,sk.shape)=true limit 50;
select sk.gid,sk.dataset,co.gid from Poly_veg co,Poly_veg sk where st_touches(co.shape,sk.shape)=true and co.gid =2 and co.dataset='Trees';
select sk.OGR_FID,co.OGR_FID from Majorroads co,Majorroads sk where st_crosses(co.shape,sk.shape)=true and sk.OGR_FID=115783;
select co.gid,sk.gid from Poly_veg co,Poly_veg sk where within(co.shape,sk.shape)=true AND co.gid=51238 limit 10;
select sk.gid,co.gid from Poly_veg co,Poly_veg sk where contains(co.shape,sk.shape)=true limit 5;
select  sk.gid,co.OGR_FID from Places co,Poly_veg sk where within(co.shape,sk.shape)=true limit 100;
select sk.gid,co.OGR_FID from Places co,Poly_veg sk where intersects(co.shape,sk.shape)=true limit 100;
select sk.OGR_FID,co.OGR_FID from Places co,Majorroads sk where intersects(co.shape,sk.shape)=true and co.OGR_FID=734;
