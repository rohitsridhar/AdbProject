select w.gid,w1.gid from airports w,airports w1 where w.gid<>w1.gid and st_equals(w.geom,w1.geom)=true limit 30;
select na.gid,na1.gid from swamps na,swamps na1 where st_equals(na.geom,na1.geom)=true limit 20;
select na.gid,na1.gid from swamps na,swamps na1 where st_disjoint(na.geom,na1.geom)=true and na.gid<>na1.gid limit 1000;
select na.gid,na1.gid from swamps na,swamps na1 where st_touches(na.geom,na1.geom)=true and na.gid<>na1.gid limit 1;
select m.gid,m1.gid from maj_riv m,maj_riv m1 where st_crosses(m.geom,m1.geom)=true and m.gid=836;
select m.gid,n.gid from maj_riv m,swamps n where st_intersects(m.geom,n.geom)=true limit 5;
select n.gid,n1.gid from swamps n,swamps n1 where st_within(n.geom,n1.geom)=true limit 50;
select n.gid,n1.gid from swamps n,swamps n1 where st_contains(n.geom,n1.geom)=true limit 50;
select n.gid,a.gid from airports a,swamps n where st_within(a.geom,n.geom)=true limit 10;
select n.gid,m.gid from maj_riv m,swamps n where st_crosses(m.geom,n.geom)=true and m.gid=2322;
select n.gid,a.gid from airports a,swamps n where st_intersects(a.geom,n.geom)=true limit 1;
