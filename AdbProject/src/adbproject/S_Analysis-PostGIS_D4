select st_dimension(geom) from swamps;
select st_dimension(geom) from maj_riv;
select st_dimension(geom) from airports;
select st_area(geom) from swamps;
select max(st_area(geom)) from swamps;
select st_length(geom) from maj_riv;
select max(st_length(geom)) from maj_riv;
select st_length(geom) from maj_riv;
select st_convexhull(geom) from swamps;
select st_envelope(geom) from maj_riv;
select sk.name as airports, co.name as riv_name from maj_riv co, airports sk where St_Distance(co.geom,sk.geom)<100 limit 10000;