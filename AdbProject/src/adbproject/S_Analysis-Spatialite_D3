select dimension(geometry) from cityAreas;
select dimension(geometry) from maj_riv2;
select dimension(geometry) from minnesota_poi;
select area(geometry) from cityAreas;
select max(area(geometry)) from cityAreas;
select length(geometry) from maj_riv2;
select min(length(geometry)) from maj_riv2;
select length(geometry) from maj_riv2;
select convexhull(geometry) from cityAreas;
select envelope(geometry) from maj_riv2;
select sk.name as minn_poi, co.name as riv_name from maj_riv2 co,minnesota_poi sk where Distance(co.geometry,sk.geometry)<100 limit 10000;