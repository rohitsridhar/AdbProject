select sk.OGR_FID,co.OGR_FID from waterpoints co,waterpoints sk where equals(co.shape,sk.shape)=true  limit 50;
select  sk.OGR_FID from Crops co,Crops sk where equals(co.shape,sk.shape)=true limit 50;
select  sk.OGR_FID from Crops co,Crops sk where overlaps(co.shape,sk.shape)=true limit 50;
select  sk.OGR_FID,co.OGR_FID from Crops co,Crops sk where disjoint(co.shape,sk.shape)=true limit 50;
select  sk.OGR_FID,co.OGR_FID from Crops co,Crops sk where st_touches(co.shape,sk.shape)=true limit 50;
select sk.OGR_FID from utilityLines co,utilitylines sk where st_crosses(co.shape,sk.shape)=true and sk.OGR_FID=54878;
select co.OGR_FID,sk.OGR_FID from crops co,crops sk where st_within(co.shape,sk.shape)=true and sk.ogr_fid=92933 limit 5;
select co.OGR_FID,sk.OGR_FID from crops co,crops sk where contains(co.shape,sk.shape)=true limit 50;
select sk.OGR_FID,co.OGR_FID from waterpoints co,crops sk where st_within(co.shape,sk.shape)=true and sk.ogr_fid = 92918;
select co.OGR_FID,sk.OGR_FID from crops co,waterpoints sk where intersects(co.shape,sk.shape)=true limit 50;
select co.OGR_FID,sk.OGR_FID from utilitylines co,waterpoints sk where intersects(co.shape,sk.shape)=true limit 50;
select sk.OGR_FID as Poly from utilitylines co,crops sk where intersects(co.shape,sk.shape)=true limit 5;
